<?xml version="1.0" encoding="ASCII"?>
<ecdarText:ETSpecificationDefinition xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecdarText="http://www.itu.dk/ecdar/text/EcdarText" xsi:schemaLocation="http://www.itu.dk/ecdar/text/EcdarText java://dk.itu.ecdar.text.ecdarText.EcdarTextPackage" name="Machine">
  <body>
    <declarations>
      <declarations xsi:type="ecdarText:ETVariableDeclaration">
        <type>
          <modifiers/>
          <identifier xsi:type="ecdarText:ETClockType"/>
        </type>
        <variables name="y"/>
      </declarations>
      <declarations xsi:type="ecdarText:ETVariableDeclaration">
        <type>
          <modifiers/>
          <identifier xsi:type="ecdarText:ETOutputType"/>
        </type>
        <variables name="tea" ioType="!"/>
      </declarations>
      <declarations xsi:type="ecdarText:ETVariableDeclaration">
        <type>
          <modifiers/>
          <identifier xsi:type="ecdarText:ETInputType"/>
        </type>
        <variables name="coin"/>
      </declarations>
      <declarations xsi:type="ecdarText:ETVariableDeclaration">
        <type>
          <modifiers/>
          <identifier xsi:type="ecdarText:ETOutputType"/>
        </type>
        <variables name="cof" ioType="!"/>
      </declarations>
    </declarations>
    <initialLocation name="Idle">
      <edges target="//@body/@initialLocation">
        <io type="!">
          <expression xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.1/@variables.0"/>
        </io>
        <guard xsi:type="ecdarText:ETGreaterEqualExpression">
          <left xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.0/@variables.0"/>
          <right xsi:type="ecdarText:ETNumberLiteral" value="2"/>
        </guard>
      </edges>
      <edges target="//@body/@locations.0">
        <io>
          <expression xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.2/@variables.0"/>
        </io>
        <updates xsi:type="ecdarText:ETAssignmentExpression">
          <left xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.0/@variables.0"/>
          <right xsi:type="ecdarText:ETNumberLiteral"/>
        </updates>
      </edges>
    </initialLocation>
    <locations name="Serving">
      <invariants xsi:type="ecdarText:ETLessEqualExpression">
        <left xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.0/@variables.0"/>
        <right xsi:type="ecdarText:ETNumberLiteral" value="6"/>
      </invariants>
      <edges target="//@body/@locations.0">
        <io>
          <expression xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.2/@variables.0"/>
        </io>
      </edges>
      <edges target="//@body/@initialLocation">
        <io type="!">
          <expression xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.1/@variables.0"/>
        </io>
      </edges>
      <edges target="//@body/@initialLocation">
        <io type="!">
          <expression xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.3/@variables.0"/>
        </io>
        <guard xsi:type="ecdarText:ETGreaterEqualExpression">
          <left xsi:type="ecdarText:ETReference" target="//@body/@declarations/@declarations.0/@variables.0"/>
          <right xsi:type="ecdarText:ETNumberLiteral" value="4"/>
        </guard>
      </edges>
    </locations>
  </body>
</ecdarText:ETSpecificationDefinition>
